    pod 'TWRDownloadManager'
    pod 'AudioPlayerSwift'


  target 'freemusicTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'freemusicUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
